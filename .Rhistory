# SERVER - Lógica de la app
server <- function(input, output) {
output$grafico <- renderPlotly({
if (input$analisis == "Resultados") {
gg <- ggplot(datosCadizResultados, aes_string(x="Fecha", y=input$var_resultados, color="Resultado")) +
geom_point(size=3) +
geom_smooth(method = "lm", se=FALSE) +
theme_minimal() +
labs(title=paste("Evolución de", input$var_resultados, "por partido"))
} else if (input$analisis == "Tiros") {
gg <- ggplot(datosCadizTiros, aes_string(x=input$var_tiros, y="GF")) +
geom_point(size=3, alpha=0.7) +
geom_smooth(method="lm", se=FALSE, color="red") +
theme_minimal() +
labs(title=paste("Relación entre", input$var_tiros, "y Goles"))
} else {
gg <- ggplot(datosTirosEnContra, aes(x=Disparos, y=GF)) +
geom_point(color="blue", size=3, alpha=0.7) +
geom_smooth(method="lm", se=FALSE, color="red") +
theme_minimal() +
labs(title="Relación entre Disparos Recibidos y Goles")
}
ggplotly(gg)
})
}
# Lanzar la aplicación
shinyApp(ui = ui, server = server)
